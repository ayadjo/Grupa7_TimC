<Window x:Class="InitialProject.View.OwnerWindows.AccommodationOverviewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InitialProject.View.OwnerWindows"
        mc:Ignorable="d"
        Title="AccommodationOverviewWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="80"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="150"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label x:Name="RegisterNewAccommodation" Content="Registrujte novi smeštaj"/>
        <Button x:Name="RegistenNewAccommodationButton" Content="+" FontSize="30" Grid.Column="0" Grid.Row="0" Margin="50,25,51,5" Click="RegistenNewAccommodationButton_Click"/>
    </Grid>
</Window>
